public class Gastos {
    private String concepto;
    private double monto;

    public Gastos(String concepto, double monto) {
        this.concepto = concepto;
        this.monto = monto;
    }

    public boolean realizarGasto(Usuarios usuario) {
        if (monto > 0 && cantidad <= usuario.getSaldo()) {
            usuario.actualizarSaldo(-monto);
            return true;
        }
        return false;
    }

    public String getConcepto() {
        return concepto;
    }

    public double getCantidad() {
        return monto;
    }
}

